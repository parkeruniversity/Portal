      <Query>
        <DataSourceName>ics_net_stat</DataSourceName>
        <CommandText>SELECT     Portlet_Template, Portlet, Date_Hit, Number_Of_Hits, Visitor
FROM         dbo.SST_vwPortletUsage
WHERE     (Date_Hit BETWEEN @Start_Date AND @End_Date)</CommandText>
        <QueryParameters>
